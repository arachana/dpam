#Declaring objects with css and xpath locators
@objects
  bullet_point_section-*	xpath	//*[@class='bullet-points-section']
  section_heading-*	xpath	//*[contains(@class,'section-heading')]
  sub_content-*	xpath	//*[@class='sub-content']
  testimonial_block-*	xpath	(//*[@class='testimonail-block'])[2]
  block_author-*	xpath	(//*[@class='block-author'])[2]
  block_author_details-*	xpath	(//*[@class='author-details'])[2]
  hero_image_header	xpath	(//*[@class='banner-content row']//following::h2)[1]
  breadcrumb	xpath	//*[@class='breadcrumbs']
  explore_buttons-*	xpath	//*[@class='explore-btn']
  grid_headers1-*	xpath	//*[@class='pick-box-shadow']
  grid_headers2-*	xpath	//*[@class='brand-family-content']//following::h3
  bullet_points-*	xpath	//*[contains(@class,'bullet-points')]//li
  explore_video_buttons-*	xpath	//*[contains(@class,'explore-video-btn')]//child::span
  headline-*	xpath	//*[@class='headline']
  headline_details-*	xpath	//*[@class='headline']//following::*[@class='desc']
  bullet_point_section_brands-*	xpath	//*[@class='bullet-points-section']
  brands_fact_and_figures-*	xpath	//*[@class='fact-value']//following::span//ancestor::div[@class='fact-name' or @class='fact-value']
  dail_industries_labels-*	xpath	(//*[contains(@id,'txt-')])[position()<12]
  dail_center_text	xpath	//*[@id='c-text']
  dail_view_all_brands_links	xpath	//*[@id='view-all-brands']//child::a
  content_description-*	xpath	//*[@class='content-desc']//following::p
  stories-headings-*	xpath	//*[@class='text-title']
  stories-details-*	xpath	//*[@class='text-summary']//following::p
  quote_details-*	xpath	//*[@class='block-content']//following::p
  content_titile-*	xpath	//*[@class='content-title']
  content_description-*	xpath	//*[@class='content-description']//following::p
  kit-names-*	xpath	//*[@class='kit-name'] 

