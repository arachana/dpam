#Declaring objects with css and xpath locators
@objects
  
  journey_so_far_2017_images-* xpath (//*[@class='company-icon']//img)[position()>12 and position()<18]
  journey_so_far_2017_years-* xpath (//*[@class='company-year'])[position()>9 and position()<12]
  journey_so_far_2017_images_content-* xpath (//*[@class='company-desc'])[position()>12 and position()<18]
  #journey_so_far_2017_main_image_desktop  xpath //*[@alt='journey2017skidubai']
  #journey_so_far_2017_main_image_mobile  xpath //*[@alt='whowearejourneyskidubai']
  #journey_so_far_2017_main_image_hading xpath //*[@alt='journey2017skidubai']//following::h2[position()=1]
  #journey_so_far_2017_main_image_text_content xpath //*[@alt='journey2017skidubai']//following::h2[position()=1]//following::div[position()=1]
  journey_so_far_2017_lbl-* xpath (//*[@class='lines']//child::span)[position()>16 and position()<21]
  journey_so_far_2017_carrefour_logo  xpath //*[@alt='Carrefour2']
  journey_so_far_2017_carrefour_logo_text  xpath //*[@alt='Carrefour2']//following::p[position()=1]
= Journey So Far 2017 Section Desktop =
  #This is for Desktop
	@on desktop
      journey_so_far_2017_images-*:
        width 150 px
        height 90 px
      journey_so_far_2017_images-1:
        aligned horizontally bottom journey_so_far_2017_images-2
        aligned horizontally bottom journey_so_far_2017_images-3
      journey_so_far_2017_images-4:
        aligned horizontally bottom journey_so_far_2017_images-5
      journey_so_far_2017_years-*:
        css font-size is "34px"
        css font-family matches ".*MarselisPro-Bold, sans-serif*"
      journey_so_far_2017_years-1:
        aligned horizontally all journey_so_far_2017_years-2
        #aligned horizontally all journey_so_far_2017_years-3 
      journey_so_far_2017_images_content-*:
        css font-size is "16px"
        css font-family matches ".*DIN-Alternate, sans-serif*"
      journey_so_far_2017_images_content-1:
        aligned horizontally top journey_so_far_2017_images_content-2
        #aligned horizontally top journey_so_far_2017_images_content-3
      #journey_so_far_2017_main_image_desktop:
        #width 743 px
        #height 519 px
      #journey_so_far_2017_main_image_hading:
        #css font-size is "80px"
        #css font-family matches ".*MarselisPro-Bold, sans-serif*"
      #journey_so_far_2017_main_image_text_content:
        #css font-size is "16px"
        #css font-family matches ".*DIN-Alternate, sans-serif*"
      #journey_so_far_2017_lbl-*:
        #css font-size is "52px"
        #css font-family matches ".*MarselisPro-Bold, sans-serif*"
      journey_so_far_2017_carrefour_logo:
        width 150 px
        height 150 px
        inside screen 591px left
        inside screen 592px right
      journey_so_far_2017_carrefour_logo_text:
        css font-size is "16px"
        css font-family matches ".*DIN-Alternate, sans-serif*"
        text is "Carrefour arrives in Kenya"
        inside screen 381px left
        inside screen 383px right
