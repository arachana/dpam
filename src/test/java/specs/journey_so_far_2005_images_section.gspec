#Declaring objects with css and xpath locators
@objects
  
  journey_so_far_2005_images-* xpath (//*[@class='company-icon']//img)[position()>6 and position()<10]
  journey_so_far_2005_years-* xpath (//*[@class='company-year'])[position()>3 and position()<7]
  journey_so_far_2005_images_content-* xpath (//*[@class='company-desc'])[position()>6 and position()<10]
  journey_so_far_2005_main_image_desktop  xpath //*[text()='Ski Dubai']//preceding::img[position()=1]
  journey_so_far_2005_main_image_mobile  xpath //*[text()='Ski Dubai']//preceding::img[position()=1]
  journey_so_far_2005_main_image_hading xpath //*[text()='Ski Dubai']
  journey_so_far_2005_main_image_text_content xpath //*[text()='Ski Dubai']//preceding::img[position()=1]//following::h2[position()=1]//following::div[position()=1]
  journey_so_far_2005_lbl-* xpath (//*[@class='lines']//child::span)[position()>8 and position()<13]
  journey_so_far_2005_mall_of_emirates_logo  xpath //*[@alt='MallOfEmirates']
  journey_so_far_2005_mall_of_emirates_logo_text  xpath //*[@alt='MallOfEmirates']//following::p[position()=1]
= Journey So Far 2005 Section Desktop =
  #This is for Desktop
	@on desktop
      journey_so_far_2005_images-*:
        width 150 px
        height 90 px
      journey_so_far_2005_images-1:
        aligned horizontally bottom journey_so_far_2005_images-2
        aligned horizontally bottom journey_so_far_2005_images-3
      journey_so_far_2005_years-*:
        css font-size is "34px"
        css font-family matches ".*MarselisPro-Bold, sans-serif*"
      journey_so_far_2005_years-1:
        aligned horizontally all journey_so_far_2005_years-2
        aligned horizontally all journey_so_far_2005_years-3 
      journey_so_far_2005_images_content-*:
        css font-size is "16px"
        css font-family matches ".*DIN-Alternate, sans-serif*"
      journey_so_far_2005_images_content-1:
        aligned horizontally top journey_so_far_2005_images_content-2
        aligned horizontally top journey_so_far_2005_images_content-3
      journey_so_far_2005_main_image_desktop:
        width 743 px
        height 519 px
      journey_so_far_2005_main_image_hading:
        css font-size is "80px"
        css font-family matches ".*MarselisPro-Bold, sans-serif*"
      journey_so_far_2005_main_image_text_content:
        css font-size is "16px"
        css font-family matches ".*DIN-Alternate, sans-serif*"
      #journey_so_far_2005_lbl-*:
        #css font-size is "52px"
        #css font-family matches ".*MarselisPro-Bold, sans-serif*"
      journey_so_far_2005_mall_of_emirates_logo:
        width 150 px
        height 150 px
        inside screen 591px left
        inside screen 592px right
      journey_so_far_2005_mall_of_emirates_logo_text:
        css font-size is "16px"
        css font-family matches ".*DIN-Alternate, sans-serif*"
        #text is "The regionâ€™s 1st shopping resort & entertainment centre"
        inside screen 381px left
        inside screen 383px right
